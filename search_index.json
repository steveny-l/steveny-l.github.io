roupadd","/usr/sbin/useradd","/var/lib/mysql","0.0.0.0","1.0.2r","1.0.2r.tar.gz","1.14.2","1.14.2.tar.gz","1.2.11","1.2.11.tar.gz","1、check","1、open","2728","2、init","2、restart","2、set","3.noarch.rpm","3、reset","3、set","3、write","4.0.9","4.0.9.tgz","4、open","4、save","4、start","5、check","7.4","8.43","8.43.tar.gz","9","=","[mysqld]","\\","add","authentication_string=password('11232123')","autoconf","automak","aux","bind_ip","c++","cd","center/commun","cento","check","chown","commun","conf","config","data/mongodb","data/mongodb/db","data/mongodb/log","dbpath","el7","env","exit;","flush","fork","ftp://ftp.csx.cam.ac.uk/pub/software/programming/pcre/","ftp://ftp.csx.cam.ac.uk/pub/software/programming/pcre/pcr","ftp://ftp.openssl.org/source/","g","g++","gcc","grant","http://nginx.org/download/nginx","http://nginx.org/en/download.html","http://repo.mysql.com/mysql80","http://www.openssl.org/source/openssl","http://www.zlib.net/","http://zlib.net/zlib","http_ssl_modul","https://dev.mysql.com/downloads/repo/yum/","https://fastdl.mongodb.org/linux/mongodb","https://www.mongodb.com/download","initi","instal","ivh","jurisdict","kill","libtool","linux","logpath","make","mkdir","mongodb","mongodb.conf","mv","my.cnf,","mysql","mysql80","mysql:mysql","mysqld","nginx","note","openssl","openssl=/usr/local/src/openssl","other","p","password","path=/usr/local/nginx/nginx","path=/usr/local/src/nginx/nginx.conf","path=/usr/local/src/nginx/nginx.pid","pcre","pcre=/usr/local/src/pcr","pid","port","privileges;","ps","r","releas","remov","reset","restart","rpm","run","sbin","server","servic","set","skip","start","statu","steveni","systemctl","tabl","tar","true","up","updat","user","user='root';","version","vi","welcom","wget","www","x86_64","y","yum","zlib","zlib=/usr/local/src/zlib","zxvf","|grep","–config"],"pipeline":["stopWordFilter","stemmer"]},"store":{"./":{"url":"./","title":"Welcome","keywords":"","body":"Welcome to steveny notes\n"},"serve/nginx/":{"url":"serve/nginx/","title":"Nginx","keywords":"","body":"Install\n\nEnv\nCentOS 7.4\nInstall make\n  yum -y install gcc automake autoconf libtool make\nInstall g++\n  yum -y install gcc gcc-c++\nInstall pcre\nCheck pcre version\nftp://ftp.csx.cam.ac.uk/pub/software/programming/pcre/\n  cd /usr/local/src\n\n  wget ftp://ftp.csx.cam.ac.uk/pub/software/programming/pcre/pcre-8.43.tar.gz\n\n  tar -zxvf pcre-8.43.tar.gz && cd pcre-8.43\n\n  ./configure && make && make install\nInstall zlib\nCheck zlib version\nhttp://www.zlib.net/\n  cd /usr/local/src\n\n  wget http://zlib.net/zlib-1.2.11.tar.gz\n\n  tar -zxvf zlib-1.2.11.tar.gz && cd zlib-1.2.11\n\n  ./configure && make && make install\nInstall openssl\nCheck openssl version\nftp://ftp.openssl.org/source/\n  cd /usr/local/src\n\n  wget http://www.openssl.org/source/openssl-1.0.2r.tar.gz\n\n  tar -zxvf openssl-1.0.2r.tar.gz && cd openssl-1.0.2r\n\n  ./config && make && make install\nInstall nginx\nCheck nginx version\nhttp://nginx.org/en/download.html\n  cd /usr/local/src\n\n  wget http://nginx.org/download/nginx-1.14.2.tar.gz\n\n  tar -zxvf nginx-1.14.2.tar.gz && cd nginx-1.14.2\n\n  ./configure --sbin-path=/usr/local/nginx/nginx \\--conf-path=/usr/local/src/nginx/nginx.conf \\--pid-path=/usr/local/src/nginx/nginx.pid \\--with-http_ssl_module \\--with-pcre=/usr/local/src/pcre-8.43 \\--with-zlib=/usr/local/src/zlib-1.2.11 \\--with-openssl=/usr/local/src/openssl-1.0.2r\n\n  make && make install\n\n  /usr/sbin/groupadd www\n\n  /usr/sbin/useradd -g www www\nRun nginx\n  /usr/sbin/groupadd www\n\n  /usr/sbin/useradd -g www www\n\n  /usr/local/nginx/nginx\n"},"serve/mongodb/":{"url":"serve/mongodb/","title":"Mongodb","keywords":"","body":"Install\n\nEnv\nCentOS 7.4\nInstall mongodb\n1、Check mongodb version\nhttps://www.mongodb.com/download-center/community\n  cd /usr/local/src\n\n  wget https://fastdl.mongodb.org/linux/mongodb-linux-x86_64-4.0.9.tgz\n\n  tar -zxvf mongodb-linux-x86_64-4.0.9.tgz\n\n  mv mongodb-linux-x86_64-4.0.9 /usr/local/mongodb\n2、Set up config\n  cd /\n\n  mkdir -p data/mongodb/db\n\n  mkdir -p data/mongodb/logs\n\n  cd data/mongodb\n\n  vi mongodb.conf\n3、Write this config in mongodb.conf\n  dbpath = /data/mongodb/data\n  logpath = /data/mongodb/logs/mongodb.log\n  bind_ip = 0.0.0.0\n  port = 2728\n  fork = true\n4、Save & Start mongodb\n  cd /usr/local/mongodb/bin\n\n  ./mongod –config /data/mongodb/mongodb.conf\n5、Check mongodb\nps aux |grep mongodb\nothers\nkill mongodb\nkill -9 PID\n"},"serve/mysql/":{"url":"serve/mysql/","title":"Mysql","keywords":"","body":"Install\n\nEnv\nCentOS 7.4\nInstall mysql\n1、Check mysql version\nhttps://dev.mysql.com/downloads/repo/yum/\n  cd /usr/local/src\n\n  wget http://repo.mysql.com/mysql80-community-release-el7-3.noarch.rpm\n\n  rpm -ivh mysql80-community-release-el7-3.noarch.rpm\n\n  yum update\n\n  yum install mysql-server\n2、Init mysql\n  mysqld --initialize\n3、Set jurisdiction\n  chown mysql:mysql -R /var/lib/mysql\n4、Start mysql\n  systemctl start mysqld\n5、Check mysql status\nsystemctl status mysqld\nReset password\n1、Open my.cnf, and add skip-grant-tables after [mysqld]\n  vi /etc/my.cnf\n  ...\n\n  [mysqld]\n  skip-grant-tables\n\n  ...\n2、Restart mysql\n  service mysqld restart\n3、Reset password\n  mysql\n\n  update user set authentication_string=password('11232123') where user='root';\n\n  flush privileges;\n\n  exit;\n4、Open my.cnf, and remove skip-grant-tables\n  vi /etc/my.cnf\n  ...\n\n  [mysqld]\n\n  ...\n"}}}