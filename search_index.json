m/download","instal","libtool","linux","logpath","make","mkdir","mongodb","mongodb.conf","mv","nginx","note","openssl","openssl=/usr/local/src/openssl","p","path=/usr/local/nginx/nginx","path=/usr/local/src/nginx/nginx.conf","path=/usr/local/src/nginx/nginx.pid","pcre","pcre=/usr/local/src/pcr","pid","port","run","save","sbin","set","start","steveni","tar","true","up","version","vi","welcom","wget","write","www","x86_64","y","yum","zlib","zlib=/usr/local/src/zlib","zxvf","–config"],"pipeline":["stopWordFilter","stemmer"]},"store":{"./":{"url":"./","title":"Welcome","keywords":"","body":"Welcome to steveny notes\n"},"serve/nginx/":{"url":"serve/nginx/","title":"Nginx","keywords":"","body":"Install\n\nEnv\nCentOS 7.4\nInstall make\n  yum -y install gcc automake autoconf libtool make\nInstall g++\n  yum -y install gcc gcc-c++\nInstall pcre\nCheck pcre version\nftp://ftp.csx.cam.ac.uk/pub/software/programming/pcre/\n  cd /usr/local/src\n\n  wget ftp://ftp.csx.cam.ac.uk/pub/software/programming/pcre/pcre-8.43.tar.gz\n\n  tar -zxvf pcre-8.43.tar.gz && cd pcre-8.43\n\n  ./configure && make && make install\nInstall zlib\nCheck zlib version\nhttp://www.zlib.net/\n  cd /usr/local/src\n\n  wget http://zlib.net/zlib-1.2.11.tar.gz\n\n  tar -zxvf zlib-1.2.11.tar.gz && cd zlib-1.2.11\n\n  ./configure && make && make install\nInstall openssl\nCheck openssl version\nftp://ftp.openssl.org/source/\n  cd /usr/local/src\n\n  wget http://www.openssl.org/source/openssl-1.0.2r.tar.gz\n\n  tar -zxvf openssl-1.0.2r.tar.gz && cd openssl-1.0.2r\n\n  ./config && make && make install\nInstall nginx\nCheck nginx version\nhttp://nginx.org/en/download.html\n  cd /usr/local/src\n\n  wget http://nginx.org/download/nginx-1.14.2.tar.gz\n\n  tar -zxvf nginx-1.14.2.tar.gz && cd nginx-1.14.2\n\n  ./configure --sbin-path=/usr/local/nginx/nginx \\--conf-path=/usr/local/src/nginx/nginx.conf \\--pid-path=/usr/local/src/nginx/nginx.pid \\--with-http_ssl_module \\--with-pcre=/usr/local/src/pcre-8.43 \\--with-zlib=/usr/local/src/zlib-1.2.11 \\--with-openssl=/usr/local/src/openssl-1.0.2r\n\n  make && make install\n\n  /usr/sbin/groupadd www\n\n  /usr/sbin/useradd -g www www\nRun nginx\n  /usr/sbin/groupadd www\n\n  /usr/sbin/useradd -g www www\n\n  /usr/local/nginx/nginx\n"},"serve/mongodb/":{"url":"serve/mongodb/","title":"Mongodb","keywords":"","body":"Install\n\nEnv\nCentOS 7.4\nInstall mongodb\nCheck mongodb version\nhttps://www.mongodb.com/download-center/community\n\n  cd /usr/local/src\n\n  wget https://fastdl.mongodb.org/linux/mongodb-linux-x86_64-4.0.9.tgz\n\n  tar -zxvf mongodb-linux-x86_64-4.0.9.tgz\n\n  mv mongodb-linux-x86_64-4.0.9 /usr/local/mongodb\nSet up config\n\n  cd /\n\n  mkdir -p data/mongodb/db\n\n  mkdir -p data/mongodb/logs\n\n  cd data/mongodb\n\n  vi mongodb.conf\nWrite this config in mongodb.conf\n\n  dbpath = /data/mongodb/data\n\n  logpath = /data/mongodb/logs/mongodb.log\n\n  port = 2728\n\n  fork = true\nSave & Start mongodb\n\n  cd /usr/local/mongodb/bin\n\n  ./mongod –config /data/mongodb/mongodb.conf\n"}}}